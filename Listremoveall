//removing all elements


public class ListCloning {

	public static void main(String[] args) {

		LinkedList<String> list = new LinkedList<String>();
		list.add("JAVA");
		list.add("J2EE");
		list.add("JSP");
		list.add("SERVLETS");
		list.add("JDBC");
		list.add("J2EE");
		System.out.println("list: "+list);
		System.out.println();

		list.clear();
		
		System.out.println(list);

	}
}
